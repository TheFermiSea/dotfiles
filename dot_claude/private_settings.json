{
  "env": {
    "VALIDATION_CLIENT": "sdk"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(npm run lint)",
      "Bash(npm run test:*)",
      "Bash(npm test:*)",
      "Bash(git status)",
      "Bash(git diff:*)",
      "Bash(git log:*)",
      "Bash(git add:*)",
      "Bash(git commit:*)",
      "Bash(git push:*)",
      "Bash(git config:*)",
      "Bash(git tag:*)",
      "Bash(git branch:*)",
      "Bash(git checkout:*)",
      "Bash(git stash:*)",
      "Bash(git fetch:*)",
      "Bash(git worktree:*)",
      "Bash(jq:*)",
      "Bash(node:*)",
      "Bash(which:*)",
      "Bash(pwd)",
      "Bash(ls:*)",
      "Bash(find:*)",
      "Bash(cargo build:*)",
      "Bash(cargo test:*)",
      "Bash(cargo check:*)",
      "Bash(cargo clippy:*)",
      "Bash(cargo fmt:*)",
      "Bash(cargo run:*)",
      "Bash(cargo nextest:*)",
      "Bash(cargo doc:*)",
      "Bash(rust-analyzer:*)",
      "Bash(ruff:*)",
      "Bash(pytest:*)",
      "Bash(uv:*)",
      "Bash(python:*)",
      "Bash(sg:*)",
      "Bash(gh pr:*)",
      "Bash(gh issue:*)",
      "Bash(gh repo:*)",
      "Bash(bd:*)",
      "Bash(du:*)",
      "Bash(wc:*)",
      "Bash(head:*)",
      "Bash(tail:*)",
      "Bash(cat:*)",
      "Bash(mkdir:*)",
      "Bash(touch:*)",
      "Bash(cp:*)",
      "Bash(mv:*)",
      "Bash(echo:*)",
      "Bash(time:*)",
      "Bash(timeout:*)",
      "Bash(ssh:*)",
      "Bash(scp:*)",
      "mcp__jules__*"
    ],
    "deny": [
      "Bash(rm -rf /)",
      "Bash(rm -rf ~)",
      "Bash(rm -rf /*)",
      "Bash(chmod 777:*)",
      "Bash(curl * | sh)",
      "Bash(wget * | sh)"
    ],
    "ask": [],
    "defaultMode": "default"
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "branch=$(git rev-parse --abbrev-ref HEAD 2>/dev/null); if [ \"$branch\" = \"main\" ] || [ \"$branch\" = \"master\" ]; then echo 'BLOCK: Cannot edit files on main/master branch. Create a feature branch first.' >&2; exit 1; fi"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "bd prime"
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "echo 'Success'"
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/briansquires/.claude/log-session-usage.sh \"$CLAUDE_TRANSCRIPT_PATH\""
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "/Users/briansquires/.claude/statusline-command.sh"
  },
  "enabledPlugins": {
    "beads@beads-marketplace": true,
    "agent-sdk-dev@claude-plugins-official": true,
    "cocoindex-skills@cocoindex": true,
    "code-review@claude-code-plugins": true,
    "commit-commands@claude-code-plugins": true,
    "document-skills@anthropic-agent-skills": true,
    "feature-dev@claude-plugins-official": true,
    "hookify@claude-code-plugins": true,
    "jules@local": true,
    "plugin-dev@claude-code-plugins": true,
    "pyright-lsp@claude-plugins-official": true,
    "pyright@claude-code-lsps": true,
    "ralph-loop@claude-plugins-official": true,
    "rust-analyzer-lsp@claude-plugins-official": true,
    "rust-analyzer@claude-code-lsps": false,
    "superpowers-lab@superpowers-marketplace": true,
    "taches-cc-resources@taches-cc-resources": true,
    "context7@claude-plugins-official": true,
    "asta@local": true,
    "code-simplifier@claude-plugins-official": true,
    "ast-grep@ast-grep-marketplace": true,
    "ralph-beads@local": true
  },
  "alwaysThinkingEnabled": true,
  "gitAttribution": false,
  "model": "haiku"
}
